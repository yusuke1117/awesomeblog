<% unless current_user?(@user) %> <%# only logged_in and current_users are able to FOLLOW and UNFOLLOW %>
    <div id="follow_form">
        <% if current_user.following?(@user) %>
            <%= render 'unfollow'%>
        <% else %>
            <%= render 'follow' %>
        <% end %>
    </div>
<% end %>